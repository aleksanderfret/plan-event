{
  "name": "plan-event-server",
  "description": "Plan An Event - Server",
  "version": "0.1.0",
  "author": {
    "name": "Aleksander Fret",
    "email": "olekfret80@gmail.com",
    "url": "https://github.com/alemikolo/"
  },
  "license": "MIT",
  "main": "src/index.ts",
  "homepage": "https://github.com/alemikolo/plan-event#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alemikolo/plan-event.git"
  },
  "bugs": {
    "url": "https://github.com/alemikolo/plan-event/issues"
  },
  "directories": {
    "src": "src",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc --build tsconfig.prod.json",
    "check": "npm-run-all lint format",
    "dev": "ts-node-dev --project ./tsconfig.json --respawn --transpileOnly ./src/index.ts",
    "format": "prettier --check \"src/**/*.{js,ts}\"",
    "format:fix": "prettier --write \"src/**/*.{js,ts}\"",
    "lint": "eslint . --ext \".js,.ts\"",
    "lint:fix": "eslint ./ --ext \".js,.ts\" --max-warnings=0 --fix",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "module-alias": "2.2.2",
    "mysql2": "2.0.0",
    "typeorm": "0.2.20"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.1",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/node": "12.12.5",
    "@typescript-eslint/eslint-plugin": "2.6.0",
    "@typescript-eslint/parser": "2.6.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "lint-staged": "9.4.2",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.6.4"
  },
  "_moduleAliases": {
    "@common": "src/common",
    "@modules": "src/modules"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}, !*.min.js": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}, !*.min.js": [
      "eslint -c .eslintrc --fix",
      "echo test",
      "git add"
    ]
  }
}
