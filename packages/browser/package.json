{
  "name": "plan-event-browser",
  "description": "Plan An Event - Browser",
  "version": "0.1.0",
  "author": "Aleksander Fret",
  "license": "ISC",
  "main": "src/app/index.tsx",
  "homepage": "https://github.com/alemikolo/plan-event#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alemikolo/plan-event.git"
  },
  "bugs": {
    "url": "https://github.com/alemikolo/plan-event/issues"
  },
  "directories": {
    "src": "src",
    "test": "tests"
  },
  "scripts": {
    "build": "webpack --progress --config ./webpack.config.prod.js",
    "dev": "webpack-dev-server --open --config ./webpack.config.dev.js",
    "check": "npm-run-all lint format stylelint",
    "format": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint . --ext \".js,.jsx,.ts,.tsx\"",
    "lint:fix": "eslint ./ --ext \".js,.jsx,.ts,.tsx\" --max-warnings=0 --fix",
    "precommit": "lint-staged",
    "stylelint": "stylelint src/**/*.scss --cache",
    "stylelint:fix": "stylelint src/**/*.scss --cache --fix"
  },
  "dependencies": {
    "react": "16.11.0",
    "react-dom": "16.11.0"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "3.2.1",
    "@types/mini-css-extract-plugin": "0.8.0",
    "@types/optimize-css-assets-webpack-plugin": "5.0.1",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "@types/terser-webpack-plugin": "1.2.1",
    "@types/webpack": "4.39.5",
    "@types/webpack-dev-server": "3.1.7",
    "@typescript-eslint/eslint-plugin": "2.6.0",
    "@typescript-eslint/parser": "2.6.0",
    "autoprefixer": "9.6.5",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.2.0",
    "css-modules-typescript-loader": "3.0.1",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.1.2",
    "html-webpack-plugin": "3.2.0",
    "lint-staged": "9.4.2",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "path": "0.12.7",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "stylelint": "11.1.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-scss": "3.11.1",
    "terser-webpack-plugin": "2.2.1",
    "ts-loader": "6.2.0",
    "typescript": "3.6.4",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.9.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,scss,css,md}, !*.min.js": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}, !*.min.js": [
      "eslint -c .eslintrc --fix",
      "echo test",
      "git add"
    ],
    "**/*.scss": [
      "stylelint --syntax scss",
      "git add"
    ]
  }
}
